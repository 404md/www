{% extends 'layout.twig' %}

{% block css %}
    <link rel="stylesheet" href="{{ asset('@css/tour-ty.min.css') }}" />
{% endblock %}

{% block body %}
    <div class=" container new-page">
        <div class="pages">
            <div class="line"></div>
            <span class="text-page">
                404Moldova is a coworking space for IT businesses and IT people from Moldova. We want to empower local
                community of small companies and skilled individuals to be competitive globally.
            </span>
        </div>
    </div>
    <div class="wrapper booking" itemscope itemprop="mainContentOfPage">
        <section class="main-block">
            <div class="container">
                <div class="back-image">
                    <div class="message">
                        <h2 class="ty">Thank You!</h2>
                        <p class="center">Almost finished... We need to confirm your email address. To complete the subscription process, please click the link in the email we just sent you.</p>
                        <div class="block-icon">
                            <img class="check" src="{{ asset('@img/check_mark.svg') }}" alt="Check">
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
{% endblock %}

{% block js %}
    <script src="{{ asset('@js/tour-ty.min.js') }}"></script>
{% endblock %}
