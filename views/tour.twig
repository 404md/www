{% extends 'layout.twig' %}

{% block css %}
    <link rel="stylesheet" href="{{ asset('@css/tour.min.css') }}" />
{% endblock %}

{% block body %}
    <div class=" container new-page">
        <div class="pages">
            <div class="line"></div>
            <span class="text-page">
                404Moldova is a coworking space for IT businesses and IT people from Moldova. We want to empower local
                community of small companies and skilled individuals to be competitive globally.
            </span>
        </div>
    </div>
    <div class="wrapper booking" itemscope itemprop="mainContentOfPage">
        <section class="main-block">
            <div class="container">
                <div class="back-image">
                    <div class="booking-form">
                        <div id="mc_embed_signup">
                            <form action="//mitocgroup.us11.list-manage.com/subscribe/post?u=13a7a5fca813b378c24ec9fe3&amp;id=ad27db484c" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                                <div id="mc_embed_signup_scroll">
                                    <h2 class="heading-contact">Schedule a Visit</h2>
                                    <div class="indicates-required"><span class="asterisk">*</span> indicates required</div>
                                    <div class="mc-field-group">
                                        <div class="group">
                                            <input type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL" required>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                            <label class="label-form" for="mce-EMAIL">E-mail <span class="asterisk">*</span></label>
                                        </div>
                                    </div>
                                    <div class="mc-field-group">
                                        <div class="group">
                                            <input type="text" value="" name="FNAME" class="required" id="mce-FNAME" required>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                            <label class="label-form" for="mce-FNAME">First Name <span class="asterisk">*</span></label>
                                        </div>
                                    </div>
                                    <div class="mc-field-group">
                                        <div class="group">
                                            <input type="text" value="" name="LNAME" class="required" id="mce-LNAME" required>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                            <label class="label-form" for="mce-LNAME">Last Name <span class="asterisk">*</span></label>
                                        </div>
                                    </div>
                                    <div class="mc-field-group size1of2">
                                        <div class="group">
                                            <input type="tel" name="TELEFON" class="required" value="" id="mce-TELEFON" required>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                            <label class="label-form" for="mce-TELEFON">Phone <span class="asterisk">*</span></label>
                                        </div>
                                    </div>
                                    <div class="mc-field-group">
                                        <div class="group">
                                            <input type="text" value="" name="JOB" class="required" id="mce-JOB" required>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                            <label class="label-form" for="mce-JOB">
                                                Specialization (QA; designer etc.) <span class="asterisk">*</span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="mc-field-group size1of2">
                                        <label class="gray-text" for="mce-VDATE-month">
                                            Choose the date you want to book your visit <span class="asterisk">*</span>
                                        </label>
                                        <div class="datefield">
                                            <span class="subfield dayfield size-input">
                                                <input class="datepart required" type="text" pattern="[0-9]*" value="" placeholder="DD" size="2" maxlength="2" name="VDATE[day]" id="mce-VDATE-day">
                                            </span> /
                                            <span class="subfield monthfield">
                                                <input class="datepart required " type="text" pattern="[0-9]*" value="" placeholder="MM" size="2" maxlength="2" name="VDATE[month]" id="mce-VDATE-month">
                                            </span> /
                                            <span class="subfield yearfield">
                                                <input class="datepart required" type="text" pattern="[0-9]*" value="" placeholder="YYYY" size="4" maxlength="4" name="VDATE[year]" id="mce-VDATE-year">
                                            </span>
                                            <span class="small-meta nowrap">( dd / mm / yyyy )</span>
                                        </div>
                                    </div>
                                    <div class="mc-field-group">
                                        <div class="group">
                                            <input type="text" value="" name="MMERGE5" id="mce-MMERGE5" required>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                            <label class="label-form" for="mce-MMERGE5">Message</label>
                                        </div>
                                    </div>
                                    <div class="mc-field-group input-group">
                                        <strong>Choose the language you prefer</strong>
                                        <ul>
                                            <li>
                                                <input type="radio" value="Romana" name="LIMBA" id="mce-LIMBA-0">
                                                <label for="mce-LIMBA-0">Romanian</label>
                                            </li>
                                            <li>
                                                <input type="radio" value="Rusa" name="LIMBA" id="mce-LIMBA-1">
                                                <label for="mce-LIMBA-1">Russian</label>
                                            </li>
                                            <li>
                                                <input type="radio" value="Engleza" name="LIMBA" id="mce-LIMBA-2">
                                                <label for="mce-LIMBA-2">English</label>
                                            </li>
                                        </ul>
                                    </div>
                                    <div id="mce-responses" class="clear">
                                        <div class="response" id="mce-error-response" style="display:none"></div>
                                        <div class="response" id="mce-success-response" style="display:none"></div>
                                    </div>
                                    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                                    <div style="position: absolute; left: -5000px;" aria-hidden="true">
                                        <input type="text" name="b_13a7a5fca813b378c24ec9fe3_ad27db484c" tabindex="-1" value="">
                                    </div>
                                    <div class="clear">
                                        <input type="submit" value="Schedule your tour" name="schedule your tour" id="mc-embedded-subscribe" class="button">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
{% endblock %}

{% block js %}
    <script src="{{ asset('@js/tour.min.js') }}"></script>
{% endblock %}
