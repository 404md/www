{
  "$": {
    "common-js": [
      "js/jquery.min.js",
      "js/jquery.validate.min.js",
      "js/aws-sdk.min.js",
      "js/main.js",
      "js/slick-1.6.0.min.js",
      "js/recaptcha-api.js",
      "js/contact.js",
      "js/footer-carousel.js",
      "js/parallax.min.js"
    ],
    "common-scss": [
      "styles/main.scss",
      "styles/responsive.scss",
      "styles/slick-1.6.0.min.css",
      "styles/slick-theme-1.6.0.min.css",
      "styles/materialdesignicons.min.css"
    ]
  },
  "routes": {
    "/": {
      "view": "index.twig",
      "vars": {
        "title": "Home page",
        "greeting": "Hello world!"
      
      },
      "assets": {
        "js/index.min.js": [
          "$.common-js"
        ],
        "css/index.min.css": [
          "$.common-scss"
        ]
      }
    },
    "/pricing": {
      "view": "pricing.twig",
      "vars": {
        "title": "Price page",
        "page": "Pricing",
        "imagePath": "img/coworking-bg.jpg" 
         },
      "assets": {
        "css/pricing.min.css": [
          "$.common-scss",
          "styles/pricing.scss"
        ],
        "js/pricing.min.js": [
          "$.common-js",
          "js/coworking.js"
        ]
      }
    },
    "/events": {
      "view": "events.twig",
      "vars": {
        "title": "Price page",
        "page": "Events",
        "imagePath": "img/events.jpg"
      },
      "assets": {
        "css/events.min.css": [
          "$.common-scss"
        ],
        "js/events.min.js": [
          "$.common-js",
          "js/events.js"
        ]
      }
    },
    "/members": {
      "view": "members.twig",
      "vars": {
        "title": "Price page",
        "page": "Members",
        "imagePath": "img/members.jpg"
      },
      "assets": {
        "css/members.min.css": [
          "$.common-scss",
          "styles/members.scss"
        ],
        "js/members.min.js": [
          "$.common-js"
        ]
      }
    },
    "/media": {
      "view": "media.twig",
      "vars": {
        "title": "Media",
        "page": "Media",
        "hideParallax": true
      },
      "assets": {
        "css/media.min.css": [
          "$.common-scss",
          "styles/media.scss"
        ],
        "js/media.min.js": [
          "$.common-js"
        ]
      }
    },
    "/gallery": {
      "view": "gallery.twig",
      "vars": {
        "title": "Price page",
        "greeting": "Quickly build an effective pricing table for your potential customers with this Bootstrap example. It's built with default Bootstrap components and utilities with little customization."
      },
      "assets": {
        "css/gallery.min.css": [
          "$.common-scss"
        ],
        "js/gallery.min.js": [
          "$.common-js"
        ]
      }
    },
    "/contact": {
      "view": "contact.twig",
      "vars": {
        "title": "Price page",
        "greeting": "Quickly build an effective pricing table for your potential customers with this Bootstrap example. It's built with default Bootstrap components and utilities with little customization."
      },
      "assets": {
        "css/contact.min.css": [
          "$.common-scss"
        ],
        "js/contact.min.js": [
          "$.common-js"
        ]
      }
    },
    "/tour": {
      "view": "tour.twig",
      "vars": {
        "title": "Price page",
        "greeting": "Quickly build an effective pricing table for your potential customers with this Bootstrap example. It's built with default Bootstrap components and utilities with little customization."
      },
      "assets": {
        "css/tour.min.css": [
          "$.common-scss",
          "styles/tour.scss"
        ],
        "js/tour.min.js": [
          "$.common-js"
        ]
      }
    }
  }
}
